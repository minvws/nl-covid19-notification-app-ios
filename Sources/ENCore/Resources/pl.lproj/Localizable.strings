/* MARK: - Chance of contamination */
"contamination.title" = "Możliwość zarażenia";

"contamination.near_someone_with_corona.title" = "Znajdowałeś się w pobliżu osoby z koronawirusem";
"contamination.near_someone_with_corona.description" = "%@ mogłeś zostać zarażony. Tego dnia znajdowałeś się przez co najmniej 15 minut w pobliżu osoby zarażonej koronawirusem.<br><br>Jeśli rzeczywiście jesteś zarażony, możesz przekazywać wirusa innym. Nawet jeśli teraz nie czujesz się chory.";

"contamination.what_to_do.title" = "Co możesz teraz zrobić?";
"contamination.what_to_do.step1" = "Pozostań w domu do %@ włącznie i zwróć szczególną uwagę na dolegliwości typowe dla koronawirusa";
"contamination.what_to_do.step2" = "Masz (łagodne) dolegliwości, które są typowe dla koronawirusa? Zrób test na obecność koronawirusa i pozostań w domu do %@ włącznie";
"contamination.what_to_do.step3" = "Masz poważne dolegliwości tj. wysoką gorączkę lub trudności z oddychaniem? Albo znajdujesz się w grupie wysokiego ryzyka i masz gorączkę? W pierwszej kolejności zadzwoń do swojego lekarza rodzinnego.";
"contamination.what_to_do.description" = "GGD w rozmowie telefonicznej podaje inne zalecenia niż aplikacja? Postępuj zgodnie z zaleceniami GGD.";

"contamination.stay_home.title" = "Pozostań w domu do %@ włącznie";
"contamination.stay_home.step1" = "Niech inni zrobią za Ciebie zakupy. Naprawdę nikt nie może tego zrobić, a Ty nie masz żadnych dolegliwości? W takim razie możesz wyjść, aby zrobić zakupy. Zachowuj 1,5 metra odległości i nikogo nie dotykaj.";
"contamination.stay_home.step2" = "Możesz przebywać w ogrodzie lub siedzieć na balkonie.";
"contamination.stay_home.step3" = "Dzieci w wieku szkolnym poniżej 18 roku życia mogą chodzić do szkoły lub uprawiać sport.";

"contamination.visitors.title" = "Nie przyjmuj gości";
"contamination.visitors.step1" = "Wyłącznie osoby, z którymi mieszkasz, mogą przebywać razem z Tobą w mieszkaniu. Trzymajcie się z dala od siebie i zachowujcie 1,5 metra odległości. Należy się powstrzymać od przytualania, całowania i uprawiania seksu.";

"contamination.medical_help.title" = "Potrzebujesz pomocy medycznej?";
"contamination.medical_help.step1" = "W pierwszej kolejności zadzwoń do swojego lekarza rodzinnego zamiast iść do przychodni lub szpitala. Poinformuj lekarza o kontakcie z osobą zarażoną koronawirusem.";
"contamination.medical_help.step2" = "Twój lekarz rodzinny lub inna pomoc medyczna może przyjść do Ciebie.";

"contamination.after.title" = "Po %@";
"contamination.after.description" = "Do %@ włącznie nie pojawiły się żadne dolegliwości? A więc nie musisz dłużej pozostawać w domu. Jednak trzymaj się ogólnych zasad zapobiegających koronawirusowi.";

